{
  "manifest_version": 3,
  "name": "Browser Cache Clearer",
  "version": "1.0.0",
  "permissions": [
    "storage",
    "browsingData",
    "scripting",
    "tabs",
    "webNavigation"
  ],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  }
}
