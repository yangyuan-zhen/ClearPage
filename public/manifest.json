{
  "manifest_version": 3,
  "name": "__MSG_appName__",
  "version": "1.0.8",
  "description": "__MSG_appDescription__",
  "default_locale": "zh_CN",
  "permissions": [
    "browsingData",
    "activeTab",
    "tabs",
    "scripting",
    "storage",
    "alarms"
  ],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "shovel.png",
      "48": "shovel.png",
      "128": "shovel.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "shovel.png",
    "48": "shovel.png",
    "128": "shovel.png"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "popup.html",
        "landing.html",
        "i18n.js",
        "browser-detect.js",
        "landing-scripts.js",
        "shovel.png",
        "wx.png",
        "zfb.jpg",
        "ava/*.jpg",
        "locales/*.json"
      ],
      "matches": ["<all_urls>"]
    }
  ]
}
