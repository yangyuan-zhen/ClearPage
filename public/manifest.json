{
  "manifest_version": 3,
  "name": "__MSG_appName__",
  "version": "1.0.1",
  "description": "__MSG_appDescription__",
  "default_locale": "zh_CN",
  "permissions": [
    "browsingData",
    "activeTab",
    "storage",
    "tabs",
    "scripting",
    "history"
  ],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "shovel.png",
      "48": "shovel.png",
      "128": "shovel.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "shovel.png",
    "48": "shovel.png",
    "128": "shovel.png"
  }
}
